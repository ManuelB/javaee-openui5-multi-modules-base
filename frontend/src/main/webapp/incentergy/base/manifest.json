{
	"_version": "1.1.0",
	"sap.app": {
		"_version": "1.1.0",
		"id": "incentergy.base",
		"type": "application",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"Data.svc": {
				"uri": "../../base-backend/Data.svc?sap-context-token=1",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_fiori_3"
		]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "incentergy.base.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.68.1",
			"libs": {
				"sap.m": {
					
				},
				"sap.f": {
					
				},
				"sap.tnt": {
					
				},
				"sap.ui.unified": {
				},
				"sap.ui.layout": {
					
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "incentergy.base.i18n.i18n"
				}
			},
			"": {
				"dataSource": "Data.svc",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "incentergy.base.view",
				"controlId": "navContainer",
				"controlAggregation": "pages",
				"clearTarget": true,
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				}
			},
			"routes": [ {
				  "pattern": ":module*:",
				  "name": "module"
				}
			],
			"targets": {
			}
		}
	}
}
